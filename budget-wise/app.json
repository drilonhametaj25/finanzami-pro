{
  "expo": {
    "name": "FinanzaMi.pro",
    "slug": "budget-wise",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "budgetwise",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#8B5A7A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.drilonhametaj.budgetwise",
      "buildNumber": "2",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Permette di allegare foto degli scontrini alle transazioni",
        "NSCameraUsageDescription": "Permette di scattare foto degli scontrini",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.drilonhametaj.budgetwise",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#8B5A7A"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-web-browser"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "632a8c05-6376-4345-ac71-aa94270409f2"
      }
    }
  }
}
